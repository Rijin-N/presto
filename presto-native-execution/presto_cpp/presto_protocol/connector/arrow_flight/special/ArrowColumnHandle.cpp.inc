namespace facebook::presto::protocol::afc {
ArrowColumnHandle::ArrowColumnHandle() noexcept {
  _type = "arrow-flight";
}

void to_json(json& j, const ArrowColumnHandle& p) {
  j = json::object();
  j["@type"] = "arrow-flight";
  to_json_key(
      j,
      "columnName",
      p.columnName,
      "ArrowColumnHandle",
      "String",
      "columnName");
  to_json_key(
      j, "columnType", p.columnType, "ArrowColumnHandle", "Type", "columnType");
}

void from_json(const json& j, ArrowColumnHandle& p) {
  p._type = j["@type"];
  from_json_key(
      j,
      "columnName",
      p.columnName,
      "ArrowColumnHandle",
      "String",
      "columnName");
  from_json_key(
      j, "columnType", p.columnType, "ArrowColumnHandle", "Type", "columnType");
}
} // namespace facebook::presto::protocol::afc
